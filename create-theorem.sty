    , shared counter              .value_required:n   = true
    , number~like                 .meta:n             = { shared~counter = #1 }
    , number~like                 .value_required:n   = true
    , number-like                 .meta:n             = { shared~counter = #1 }
    , number-like                 .value_required:n   = true
    , number like                 .meta:n             = { shared~counter = #1 }
    , number like                 .value_required:n   = true
