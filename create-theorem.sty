    % \tl_gset:cn { #4 ref@ #1 @name }
    %   {
    %     \tl_use:c { g_crthm_name_ #4 ref_article_singular_ #2 _ #3 _tl }
    %     \tl_use:c { g_crthm_name_ #4 ref_singular_ #2 _ #3 _tl }
    %   }
    % \tl_gset:cn { #4 ref@ #1 @name@plural }
    %   {
