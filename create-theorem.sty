    , crefname style        .value_required:n   = true
    , Crefname~style        .code:n             = { \crthm_set_name_style_Crefname:en { \l__crthm_current_env_tl } { #1 } }
    , Crefname~style        .value_required:n   = true
    , Crefname-style        .meta:n             = { Crefname~style = { #1 } }
    , Crefname-style        .value_required:n   = true
